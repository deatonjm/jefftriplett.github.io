BEGIN:VCALENDAR
VERSION:2.0
PRODID:http://jefftriplett.com/
METHOD:PUBLISH
{% for event in site.data.calendar.events limit:10 %}BEGIN:VEVENT
UID:{{ event.date | date: "%Y%m%d" }}@jefftriplett.com
ORGANIZER;CN="Jeff Triplett":MAILTO:hello@jefftriplett.com
LOCATION:Lawrence\, Kansas
SUMMARY:{{ event.title }}
CLASS:PUBLIC
DTSTART:{{ event.start_date | date: "%Y%m%d" }}T170000Z
{% if event.end_date %}DTEND:{{ event.end_date | date: "%Y%m%d" }}T190000Z{% else %}DTEND:{{ event.start_date | date: "%Y%m%d" }}T190000Z{% endif %}
DTSTAMP:{{ event.start_date | date: "%Y%m%d" }}T170000Z
END:VEVENT
{% endfor %}END:VCALENDAR
